{
    "version": 2,
    "builds": [
        {
            "src": "backend/index.js",
            "use": "@vercel/node"
        },
        {
            "src": "package.json",
            "use": "@vercel/static-build",
            "config": {
                "distDir": "build"
            }
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "/backend/index.js"
        },
        {
            "src": "^/static/(.*)",
            "dest": "/static/$1"
        },
        {
            "src": "^/favicon.ico",
            "dest": "/favicon.ico"
        },
        {
            "src": "^/asset-manifest.json",
            "dest": "/asset-manifest.json"
        },
        {
            "src": "^/manifest.json",
            "dest": "/manifest.json"
        },
        {
            "handle": "filesystem"
        },
        {
            "src": "/(.*)",
            "dest": "/index.html"
        }
    ]
}